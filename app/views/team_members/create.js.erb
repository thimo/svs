$('#teamMemberNew').modal('hide');

<% if @teams.present? %>
  $('#teams').html("<%= j render('member_allocations/teams') %>");
<% end %>

<% if @team_member.present? && @team_member.member.present? %>
  $('#available_member_<%= @team_member.member.id %>').hide()
<% end %>

<% flash.each do |message_type, message| %>
  $(function() {
    notie.alert({
      type: <%= notie_type(message_type) %>,
      text: "<%= message %>",
      time: 2
    })
  })
<% end %>
<% flash.clear %>
